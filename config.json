{
    "defaultPrograms": [ 
        {
            "command": "vim", 
            "opens": {"category": ["document", "code", "data"]},
            "options": { "editor": true }

        },
        {
            "command": "mupdf -r 150", 
            "opens": {"extension": ["pdf", "epub"] },
            "options": {}
        },
        { 
            "command": "xdg-open",
            "opens": {"category": ["image", "video", "audio" ]}, 
            "options": { "fallback": true }
        }
    ],

    "fileTypes":  {
        "document": ["txt", "doc", "docx", "md", "rtf", "html"],
        "book": ["pdf", "mobi", "epub", "djvu", "epub" ],
        "code": ["js", "ts", "c", "cc" ],
        "data": ["json", "zip", "csv", "db", "bz2", "ini", "rar" ],
        "video": ["mkv", "mp4", "wmv", "mpg", "avi", "swf"],
        "image": ["png", "jpg", "jpeg", "gif", "bmp", "svg" ],
        "audio": ["mp3", "wav"]
    }
}
