{
    "defaultPrograms": [ 
        {
            "program": "vim", 
            "opens": [{"type": ["document", "code", "data"]}],
            "options": { "editor": true }

        },
        {
            "program": "mupdf", 
            "opens": [{"ext": ["pdf"] }],
            "options": {}
        },
        { 
            "program": "xdg-open",
            "opens": [{"type": ["image", "video", "audio" ]} ],
            "options": { "fallback": true }
        }
    ],

    "fileTypes":  {
        "document": ["txt", "doc", "docx", "md", "rtf", "html"],
        "book": ["pdf", "mobi", "epub", "djvu", "epub" ],
        "code": ["js", "ts", "c", "cc" ],
        "data": ["json", "zip", "csv", "db", "bz2", "ini", "rar" ],
        "video": ["mkv", "mp4", "wmv", "mpg", "avi", "swf"],
        "image": ["png", "jpg", "jpeg", "gif", "bmp", "svg" ],
        "audio": ["mp3", "wav"]
    }
}
